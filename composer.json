{
    "name": "kerigard/laravel-utils",
    "description": "Utils for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Sidelnikov",
            "email": "kerihoros@gmail.com"
        }
    ],
    "homepage": "https://github.com/kerigard/laravel-utils",
    "keywords": [
        "laravel"
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kerigard\\LaravelUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerigard\\LaravelUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kerigard\\LaravelUtils\\UtilsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}